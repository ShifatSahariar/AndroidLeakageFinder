<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="edu.mit.icc_service_messages"> &gt;
<application android:debuggable="true" android:hardwareAccelerated="true" android:label="ActivityMessenger">
        <activity android:label="ActivityMessenger" android:name="edu.mit.icc_service_messages.ActivityMessenger">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <service android:exported="false" android:name="edu.mit.icc_service_messages.MessengerService"/>
    </application>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
</manifest>