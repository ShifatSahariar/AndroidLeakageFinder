    <fragment android:label="FavoritesFragment" android:name="com.blinkslabs.blinkist.android.feature.userlibrary.blinks.fragments.FavoritesFragment" android:id="@id/favoritesFragment" />
    <fragment android:label="AudiobookLibraryFragment" android:name="com.blinkslabs.blinkist.android.feature.userlibrary.audiobook.AudiobookLibraryFragment" android:id="@id/audiobookLibraryFragment" />
