        <fragment android:label="GroupedTextmarkersFragment" android:name="com.blinkslabs.blinkist.android.feature.userlibrary.textmarker.GroupedTextmarkersFragment" android:id="@id/groupedTextmarkersFragment">
            <action android:id="@id/action_to_textMarkerOfBookFragment" app:destination="@id/textMarkerOfBookFragment" />
        </fragment>
        <fragment android:label="TextMarkerOfBookFragment" android:name="com.blinkslabs.blinkist.android.feature.textmarkers.TextmarkerOfBookFragment" android:id="@id/textMarkerOfBookFragment">
            <argument android:name="bookId" app:argType="string" />
