    <fragment android:label="MixedLibraryPageFragment" android:name="com.blinkslabs.blinkist.android.feature.userlibrary.mixed.MixedLibraryPageFragment" android:id="@id/mixedLibraryPageFragment">
        <argument android:name="libraryPage" app:argType="com.blinkslabs.blinkist.android.feature.userlibrary.mixed.LibraryPage" />
    </fragment>
    <fragment android:label="UserCollectionsLibraryFragment" android:name="com.blinkslabs.blinkist.android.feature.userlibrary.collections.UserCollectionsLibraryPageFragment" android:id="@id/userCollectionsLibraryFragment">
