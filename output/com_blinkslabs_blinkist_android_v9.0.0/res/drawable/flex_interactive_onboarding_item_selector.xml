<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <stroke android:width="2.0dip" android:color="@color/flex_onboarding_selected_border_color" />
            <corners android:radius="2.0dip" />
            <solid android:color="?colorContainerSurface" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="2.0dip" android:color="?colorContainerSurface" />
            <corners android:radius="2.0dip" />
            <solid android:color="?colorContainerSurface" />
        </shape>
    </item>
</selector>